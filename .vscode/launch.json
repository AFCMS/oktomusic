  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "frontend: user",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/apps/frontend",
      "userDataDir": false
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "frontend: clean",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/apps/frontend",
      "userDataDir": true
    },
    {
      "type": "node",
      "request": "attach",
      "name": "backend: debug",
      "port": 9229,
      "restart": true,
    }
  ]
}
